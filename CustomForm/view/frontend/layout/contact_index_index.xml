<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="contact_index_index" />
    <head>
        <title>Contact Information</title>
    </head>
    <body>
        <referenceContainer name="content">
            <referenceBlock name="contactForm" remove="yes"/>
            <block class="Magento\Contact\Block\ContactForm" name="contactForm2" template="Litslink_CustomForm::form/form.phtml"></block>
            <block class="Magento\Contact\Block\ContactForm" name="contact_icons_block" template="Litslink_CustomForm::contacts-icon-block.phtml"></block>
        </referenceContainer>
    </body>
</page>
